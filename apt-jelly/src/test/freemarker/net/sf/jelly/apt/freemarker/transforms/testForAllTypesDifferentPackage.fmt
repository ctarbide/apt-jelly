<@file name="testForAllTypes.properties" package="net.sf.jelly.apt">
  <@forAllTypes var="type" annotation="net.sf.jelly.apt.samplesource.annotations.AnnotatedClassExample" annotationVar="annotation">
${type.qualifiedName}=visited
${type.qualifiedName}.annotation.name=${annotation.name}
